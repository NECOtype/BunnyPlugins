---
import '../styles/global.css'
import '@fontsource/ibm-plex-sans-condensed';
import Head from '@components/Head';

interface Props {
	title: string
}

const { title } = Astro.props
---

<html lang="en">
	<head>
		<Head title={title}/>
	</head>
	<body>
		<main>
			<slot />
		</main>
	</body>
</html>